{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(vite|@vitejs)(/(.*))?$",
    "^(react|react-dom)(/(.*))?$",
    "^next(/(.*))?$",
    "^next-intl(/(.*))?$",
    "^@mantine(/(.*))?$",
    "^nuqs(/(.*))?$",
    "^@tanstack(/(.*))?$",
    "^@dnd-kit(/(.*))?$",
    "^swiper(/(.*))?$",
    "^@iconify(/(.*))?$",
    "^react-toastify(/(.*))?$",
    "^clsx(/(.*))?$",
    "<THIRD_PARTY_MODULES>",
    "^@/api",
    "^@/components",
    "^@/configs",
    "^@/context",
    "^@/dto",
    "^@/entities",
    "^@/guards",
    "^@/hooks",
    "^@/i18n",
    "^@/icons",
    "^@/mutations",
    "^@/providers",
    "^@/queries",
    "^@/reducers",
    "^@/schemas",
    "^@/sections",
    "^@/types",
    "^@/utils",
    "^@/android",
    "^@/admin",
    "^@/styles",
    "^(\\.|\\.\\.)/(.(?!.(css|scss)))*$",
    "\\.(css|scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true
}
